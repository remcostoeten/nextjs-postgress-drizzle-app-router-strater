{
  "name": "Remco Stoeten Todo app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next && rm -rf node_modules && bun i && bun run dev",
    "rebuild": "rm -rf .next && rm -rf node_modules && bun i && bun run build",
    "db": "drizzle-kit generate:pg && node ./src/server/db/migrate.mjs",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "node ./src/server/db/migrate.mjs",
    "db:run": "docker run --name postgres --rm -e POSTGRES_USER=postgres -e POSTGRES_PASSWORD=unsafeLocalhostPassword -e PGDATA=/var/lib/postgresql/data/pgdata -v /tmp:/var/lib/postgresql/data -p 5432:5432 -it postgres:16rc1-alpine"
  },
  "dependencies": {
    "@auth/core": "0.15.1",
    "@auth/drizzle-adapter": "0.3.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@formkit/auto-animate": "^0.8.0",
    "@hookform/resolvers": "3.3.1",
    "@mui/icons-material": "^5.14.15",
    "@mui/material": "^5.14.15",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@tanstack/react-query": "4.35.3",
    "@trpc/client": "10.38.4",
    "@trpc/react-query": "10.38.4",
    "@trpc/server": "10.38.4",
    "@types/node": "20.6.4",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "date-fns": "^2.30.0",
    "drizzle-orm": "0.28.6",
    "next": "13.5.2",
    "next-auth": "4.23.1",
    "next-intl": "3.0.0-beta.19",
    "postcss": "8.4.30",
    "postgres": "3.3.5",
